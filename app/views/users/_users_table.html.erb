<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user|%>
    <tr>
      <td>
          <% if user.profile_image.attached? %>
            <%= image_tag user.profile_image, size: "40x40" %>
          <% else %>
            <%= image_tag 'no_image', size: "40x40" %>
          <% end %>
      </td>
      <td><%= user.name %></td>
      <td><%= link_to "Show",user_path(user.id) %></td>
    </tr>
    <%end%>
  </tbody>
</table>